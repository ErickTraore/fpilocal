            <div class="text-center">
                {# <img style="margin:15px" width="120" src="{{ asset('adhesions/' ~ adhesion.imageName) }}" #}
                <img style="margin:15px" width="120" src="{{ asset('assets/adhesions/' ~ adhesion.imageName) }}"
                     alt="test">
            </div>
        {% endif %}
        {{ form_widget(form.image, {'attr':{'class':'form-control'}}) }}
